{
    "name": "sangue-conecta",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "watch:sass": "sass ./sass/index.scss ./css/style.min.css -w",
        "compile:sass": "sass ./sass/index.scss ./css/style.comp.css",
        "concat:sass": "concat -o ./css/style.concat.css ./css/icons.css ./css/style.comp.css",
        "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' ./css/style.concat.css -o ./css/style.prefix.css",
        "compress:css": "sass ./css/style.prefix.css ./css/style.min.css --output-style compressed",
        "build:css": "npm-run-all compile:sass concat:sass prefix:css compress:css"
    },
    "author": "Murilo-Sanches",
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "autoprefixer": "^10.4.21",
        "concat": "^1.0.3",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^11.0.1",
        "sass": "^1.89.1"
    }
}
